# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'shine_for-iOS' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'RxCocoa'
  pod 'RxSwift'
  pod 'Firebase/Core'
  pod 'FirebaseDatabase'
  pod 'FirebaseAuth'
  pod 'Firebase/Storage'
  pod 'SVProgressHUD'


  # Pods for shine_for-iOS

  target 'shine_for-iOSTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'shine_for-iOSUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
